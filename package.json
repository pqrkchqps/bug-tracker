{
  "name": "bug-tracker",
  "version": "0.0.1",
  "description": "A simple bug tracker with users and admins",
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd react-frontend; npm run build",
    "start": "node ./server/server.js",
    "install": "cd react-frontend; npm install",
    "client": "npm start --prefix react-frontend",
    "server": "nodemon ./server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pqrkchqps/bug-tracker.git"
  },
  "keywords": [
    "bug",
    "tracker",
    "admin",
    "controls"
  ],
  "author": "Aaron Sawyer, John Creps",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pqrkchqps/bug-tracker/issues"
  },
  "homepage": "https://github.com/pqrkchqps/bug-tracker#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg-promise": "^10.6.1"
  },
  "devDependencies": {}
}
